<html>
<head>
    <title>{{title}} (Under Construction)</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    {{> scripts-and-sheets }} 
</head>
<body class="no-sidebar">

<!-- Header -->
<div id="header-wrapper">
    <div id="header" class="container">

        <!-- Nav -->
        {{> nav}}
    </div>
</div>

<!-- Main -->
<div class="wrapper">
    <div class="container" id="main">

        <!--Product Header -->
        <header>
            <h2>{{title}}</h2>
            <p>
                {{summary}}
            </p>
        </header>
	{{> body }}
    </div>
</div>

<div class="wrapper">
    <div class="container" id="sub2">

        <!-- Content -->
        <article id="content">
            <header style="text-align: center">
                <h2>This Page is Currently Under Construction</h2>
                <p>Please leave your contact information so we can inform you when we're done building</p>
            </header>

            <section style="text-align: center; font-size: 2em">
                <ul class="actions">
                    <li><a href="#footer-wrapper" class="button">Contact</a></li>
                </ul>
            </section>

        </article>

    </div>
</div>

<!-- Footer -->
{{> footer}}

</body>
</html>
